{
  "version": 2,
  "builds": [
    {
      "src": "FrontEnd/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "FrontEnd/dist"
      }
    },
    {
      "src": "BackEnd/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/BackEnd/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/FrontEnd/dist/$1"
    }
  ]
}
